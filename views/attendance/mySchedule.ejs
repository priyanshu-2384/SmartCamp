<% layout("layouts/boilerplate") %>

<div class="container ">
    <h1 class="mt-5 texts">Today's Schedule of <%= currUser.username %>
    </h1>
    <table class="table table-striped table-dark mt-5">
      <thead class="thead-light">
        <tr>
          <th>Subject</th>
          <th>Time</th>
        </tr>
      </thead>
      <tbody>
        <% chat1.forEach(item=> { %>
            <tr>
              <td>
                <%= item.Subject %>
              </td>
              <td>
                <%= item.time %>
              </td>
            </tr>
            <% }) %>  
        
      </tbody>
    </table>
  </div>
